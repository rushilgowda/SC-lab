      ],
      "source": [
        "import numpy as np\n",
        "X = np.array([[1, 1], [1, -1], [-1, 1], [-1, -1]])\n",
        "Y = np.array([1, -1, -1, -1])\n",
        "W = np.zeros(2)\n",
        "for i in range(len(X)):\n",
        "    W += X[i] * Y[i]\n",
        "print(\"Weight vector using Hebb's Rule:\", W)"
      ]
    }
  ]
